<%= simple_form_for [@list, @bookmark] do |f| %>
  <%= f.input :comment %>
  <%= f.association :movie %>
  <div class="d-flex justify-content-center">
    <%= f.button :submit, class: "btn btn-primary mx-3" %>
    <%= link_to "Return to list", list_path(@list), class: "btn btn-secondary mx-3"%>
  </div>
<% end %>
